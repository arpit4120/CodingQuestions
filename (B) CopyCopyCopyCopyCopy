//(B) CopyCopyCopyCopyCopy
//codeforces.com
#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define FAST ios_base::sync_with_stdio(false);cin.tie(NULL);
int32_t main() {
    ll t;
    FAST;
    cin>>t;
    while(t--)
    {
    	int n;
    	cin>>n;
    	set<int>s;
    	for(int i=0;i<n;i++)
    	{
    		int a;
    		cin>>a;
    		s.insert(a);            
    	}
    	cout<<s.size()<<endl;
    	
    	
    }
    return 0;
}
