//Splitting candles
//codechef.com
#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define FAST ios_base::sync_with_stdio(false);cin.tie(NULL);
int32_t main() {
    ll t;
    FAST;
    cin>>t;
    while(t--)
    {
    	ll  n,k;
    	cin>>n>>k;
    	if(k==0)
    		cout<<0<<" "<<n<<endl;
    	else
    		cout<<n/k<<" "<<n%k<<endl;
    	
    	
    }
    return 0;
}
