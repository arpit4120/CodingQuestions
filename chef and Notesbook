//chef and notebook
//codechef.com
#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define FAST ios_base::sync_with_stdio(false);cin.tie(NULL);
int32_t main() {
    ll t;
    FAST;
    cin>>t;
    while(t--)
    {
    	ll x,y,k,n;
    	cin>>x>>y>>k>>n;
    	x=x-y;
    	ll l=0;
    	for(int i=0;i<n;i++)
    	{
    		ll p,c;
    		cin>>p>>c;
    		if(l==0 and p>=x and c<=k)
    		{
    			l=1;
    			
    		}
    	}
    	if(l==1)
    		cout<<"LuckyChef"<<endl;
    	else
    		cout<<"UnluckyChef"<<endl;
    	
    	
    }
    return 0;
}
