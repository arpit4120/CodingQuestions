//  Number Game Revisited
//codechef.com
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    long long int t;
    cin>>t;
    while(t--)
    {
        long long int n;
        cin>>n;
        long long int p=(n%4);
        if( p == 0 || p == 2 || p == 3)
        {
             cout<<"BOB"<<endl;
        }
        else
        {
            cout<<"ALICE"<<endl;
        }
    }
}
